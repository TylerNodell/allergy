

<h1>Create Recipe </h1>
<% @recipe.errors.full_messages.each do|msg| %>
  <%= msg %>
<% end %>
<%= form_for @recipe do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br/>
  <%= f.collection_select :user_id, User.all, :id, :name, {:include_blank => true} %>
  <br/>
  <%= f.submit %>
<% end %>
